#!/bin/bash
 
for run in {1..10}
do
   date +"%H:%M:%S"
   ps -f | wc -l
   sleep 5
done
 
  GNU nano 4.9.3                      sleeper.sh                                
#!/bin/bash
 
for run in {1..10}
do
   date +"%H:%M:%S"
ps -f | wc -l
   sleep 0.3
done
 

  GNU nano 4.9.3                     osscript.sh                                
#!/bin/bash
 
cat /etc/os-release | head -1 > osinfo.txt
 
  GNU nano 4.9.3                   alponlyscript.sh                             
#!/bin/bash
  GNU nano 4.9.3                   alponlyscript.sh                             
#!/bin/bash
cat /etc/os-release | head -1 | grep -o Alpine > AlpineOnly.txt
  GNU nano 4.9.3                   50filesScript.sh                             
#!/bin/bash
 
for run in {50..100}
do
   > file$run.txt
done

localhost:~# chmod 744 50filesScript.sh
localhost:~# ./50filesScript.sh
